<Window x:Class="CollabAPIMEP.MainWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CollabAPIMEP"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="150">
    <Grid>
        <Button x:Name="EnableFamLoaded" Content="Enable" Margin="10" HorizontalAlignment="left" VerticalAlignment="Center" Click="EnableFamLoaded_Click"></Button>
        <Button x:Name="DisableFamLoaded" Content="Disable" Margin="10" HorizontalAlignment="Right" VerticalAlignment="Center" Click="DisableFamLoaded_Click"></Button>
            
    </Grid>
</Window>
